{
    "name": "dynamic/youtube-integration",
    "description": "YouTube api integration module allowing for including playlists and individual videos on pages.",
    "authors": [
        {
            "name": "Nic Horstmeier",
            "email": "nhorstmeier@dynamicagency.com"
        },
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "http://www.dynamicagency.com"
        }
    ],
    "keywords": [
        "silverstripe",
        "youtube"
    ],
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "require": {
        "google/apiclient": "^2.2",
        "madcoda/php-youtube-api": "^1.2",
        "symbiote/silverstripe-gridfieldextensions": "^3.2",
        "silverstripe/recipe-cms": "^4.2@RC"
    },
    "extra": {
        "installer-name": "silverstripe-youtube-integration",
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "config": {
            "process-timeout": 600
        }
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\YouTubeIntegration\\": "src/",
            "Dynamic\\YouTubeIntegration\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
