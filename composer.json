{
  "name": "dynamic/youtube-integration",
  "description": "YouTube api integration module allowing for including playlists and individual videos on pages.",
  "authors": [
    {
      "name": "Nic Horstmeier",
      "email": "nhorstmeier@dynamicagency.com"
    },
    {
      "name": "Dynamic",
      "email": "dev@dynamicagency.com",
      "homepage": "http://www.dynamicagency.com"
    }
  ],
  "keywords": [
    "silverstripe",
    "youtube"
  ],
  "type": "silverstripe-module",
  "license": "BSD-3-Clause",
  "require": {
    "silverstripe/framework": "^3.2",
    "google/apiclient": "^v2.0",
    "madcoda/php-youtube-api": "^v1.1",
    "symbiote/silverstripe-gridfieldextensions": "^1.4",
    "unclecheese/betterbuttons": "^1.3"
  },
  "extra": {
    "installer-name": "youtube-integration",
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "config": {
      "process-timeout": 600
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  },
  "replace": {
		"muskie9/youtube-integration": "self.version"
	}
}
